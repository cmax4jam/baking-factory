<% if current_user.role == 'admin' %>
    <%= render partial: "admin_index", locals: {breads: @breads, muffins: @muffins, pastries: @pastries, inactive_items: @inactive_items} %>

<% elsif current_user.role == 'customer' %>
    <%= render partial: "customer_index", locals: {breads: @breads, muffins: @muffins, pastries: @pastries, inactive_items: @inactive_items}  %>
<% end %>
